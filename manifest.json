{
  "name": "vertex",
  "vendor": "vtex",
  "version": "0.0.1",
  "title": "VERTEX Cloud integration",
  "description": "",
  "mustUpdateAt": "2022-08-28",
  "scripts": {
    "postreleasy": "vtex publish"
  },
  "dependencies": {
    "vtex.styleguide": "9.x"
  },
  "builders": {
    "react": "3.x",
    "admin": "0.x",
    "messages": "1.x",
    "node": "6.x",
    "graphql": "1.x",
    "docs": "0.x"
  },
  "policies": [
    {
      "name": "update-app-settings"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/checkout/pvt/configuration/orderForm"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "auth.vertexsmb.com",
        "path": "/identity/connect/token"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "restconnect.vertexsmb.com",
        "path": "/vertex-restapi/v1/sale"
      }
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
